{
  "name": "8weike",
  "version": "1.0.0",
  "description": "8weike matches talent with opportunities, builds up personal experiences and fulfills individual dreams. ",
  "main": "index.js",
  "scripts": {
    "start": "if [ \"$NODE_ENV\" = \"production\" ]; then npm run start:prod; else npm run start:dev; fi",
    "start:dev": "npm run build:client && gulp run",
    "start:prod": "node server.bundle.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack --colors",
    "build:server": "webpack --config webpack.server.config.js --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhzhang2012/8weike-server.git"
  },
  "keywords": [
    "Weike",
    "Communication",
    "Talent"
  ],
  "author": "Tony Zhang <zhzhangtony@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zhzhang2012/8weike-server/issues"
  },
  "homepage": "https://github.com/zhzhang2012/8weike-server#readme",
  "dependencies": {
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.11.6",
    "body-parser": "^1.15.2",
    "chai": "^3.5.0",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.5.1",
    "express": "^4.14.0",
    "jquery": "^3.1.0",
    "mysql": "^2.11.1",
    "path": "^0.12.7",
    "react": "^15.3.0",
    "react-document-title": "^2.0.2",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "redux": "^3.5.2",
    "underscore": "^1.8.3",
    "webpack": "^1.13.1",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "gulp": "^3.9.1",
    "gulp-eslint": "^3.0.1",
    "http-proxy": "^1.14.0",
    "nodemon": "^1.10.0",
    "redux-devtools": "^3.3.1",
    "webpack-dev-server": "^1.14.1"
  }
